package com.technoelevate.jobportal;

import java.util.LinkedHashSet;
import java.util.Scanner;

public class CandidateDetails {
	
	Scanner sc=new Scanner (System.in);
LinkedHashSet<Candidate> linkedHashSet=new LinkedHashSet<Candidate>();
	
void createCandidate(){
	for(int i=0;i<=5;i++) {
		System.out.println("Candidate"+i+1+" please enter yor details");
	
	Candidate candidate=getInput();
	linkedHashSet.add(candidate);
	}
	
	
	
}

private  Candidate getInput() {
	System.out.println("enter your name");
	String name=sc.next();
	System.out.println("enter your 10th Percentage");
	double tenthpercentage=sc.nextDouble();
	System.out.println("Enter your PUC Percentage");
	double pucpercentage=sc.nextDouble();
	System.out.println("Enter your Degree Perecentage");
	double degreethpercentage=sc.nextDouble();
	System.out.println("Enter your YOP");
	int year=sc.nextInt();
	return new Candidate(name,tenthpercentage,pucpercentage,degreethpercentage,year);


}
}
